{
   "background_page": "background.html",
   "content_scripts": [ {
      "js": [ "/js/lib/jquery-1.4.2.min.js", "/js/content_script.js" ],
      "matches": [ 
        "http://*.facebook.com/*"
      ],
      "run_at": "document_end",
      "all_frames": true
   } ],
   "description": "Get *your* data out of Facebook, whether they want you to or not. Facebook doesn't own my friends.",
   "name": "Facebook Exporter",
   "permissions": [ 
      "unlimitedStorage",
      "tabs", 
      "http://www.google.com/m8/feeds/*", 
      "https://www.google.com/accounts/OAuthAuthorizeToken", 
      "https://www.google.com/accounts/OAuthGetAccessToken" 
    ],
   "version": "1.0"
}
